<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="movies">
    <name>Anime</name>
    <match>all</match>
    <rule field="tag" operator="contains">
        <value>anime</value>
    </rule>
    <order direction="descending">dateadded</order>
    <limit>200</limit>
</smartplaylist>
