<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!-- BINGIE -->
<smartplaylist type="tvshows">
    <name>TV Comedies</name>
    <match>all</match>
    <rule field="genre" operator="contains">
        <value>Comedy</value>
    </rule>
	<rule field="genre" operator="isnot">
		<value>Soap</value>
    </rule>
    <order direction="descending">dateadded</order>
    <limit>200</limit>
</smartplaylist>
