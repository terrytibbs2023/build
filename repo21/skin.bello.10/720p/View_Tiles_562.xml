<?xml version="1.0" encoding="UTF-8"?>
<includes> 

	<!-- ================================= -->
	<!-- ==== Tiles View Videos "562" ==== -->
	<!-- ================================= -->
	<include name="TilesView_Videos">
		<control type="group">
			<visible>Control.IsVisible(562)</visible>
			<include>CommonViews_HorizontalAnimation</include>
			<control type="image">
				<left>-50</left>
				<top>129</top>
				<width>1380</width>
				<height>475</height>
				<texture background="true" border="39">views/other/panel.png</texture>
			</control>
			<control type="fixedlist" id="562">
				<left>-355</left>
				<top>130</top>
				<width>1950</width>
				<height>475</height>
				<onleft>562</onleft>
				<onright>562</onright>
				<onup>9111</onup>
				<ondown>60562</ondown>
				<oninfo condition="$EXP[MoviesSetsCondition]">SetProperty(ShowTextViewer,1,home)</oninfo>
				<oninfo condition="$EXP[MoviesSetsCondition]">RunScript(script.embuary.helper,action=textviewer,header=$INFO[ListItem.Label],message='"$VAR[GlobalPlotDescription]"')</oninfo>
				<orientation>horizontal</orientation>
				<pagecontrol>60562</pagecontrol>
				<preloaditems>4</preloaditems>
				<focusposition>1</focusposition>
				<viewtype label="31090">icons</viewtype>
				<scrolltime tween="quadratic" easing="out">350</scrolltime>
				<visible>$EXP[TilesViewVideos_562]</visible>
				<itemlayout height="465" width="400">
					<include content="TilesViewLayout">
						<param name="titletextcolor" value="white"/>
						<param name="textcolor" value="viewsinfo"/>
						<param name="shadowcolor" value="shadowcolor"/>
						<param name="selectedcolor" value="$VAR[Text.GlobalColor]"/>
					</include>
				</itemlayout>
				<focusedlayout height="465" width="400">
					<control type="image">
						<left>0</left>
						<top>0</top>
						<width>390</width>
						<height>67</height>
						<colordiffuse>$VAR[Textures.GlobalColor]</colordiffuse>
						<texture background="true">windows/bg/white.png</texture>
						<bordersize>1</bordersize>
						<include>ThumbnailsFocusAnimation</include>
					</control>
					<control type="image">
						<left>0</left>
						<top>283</top>
						<width>390</width>
						<height>190</height>
						<colordiffuse>$VAR[Textures.GlobalColor]</colordiffuse>
						<texture background="true">windows/bg/white.png</texture>
						<bordersize>1</bordersize>
						<include>ThumbnailsFocusAnimation</include>
					</control>
					<include content="TilesViewLayout">
						<param name="titletextcolor" value="FF202020"/>
						<param name="textcolor" value="FF202020"/>
						<param name="shadowcolor" value="30FFFFFF"/>
						<param name="selectedcolor" value="FF202020"/>
						<param name="colordiffuse" value="$VAR[ColorDiffuseParentFolder]"/>
					</include>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60562">
				<left>44</left>
				<top>610</top>
				<width>1170</width>
				<height>6</height>
				<texturesliderbackground></texturesliderbackground>
				<texturesliderbar border="14,0,14,0">views/scrollbar/scrollbar_horizontal.png</texturesliderbar>
				<texturesliderbarfocus border="14,0,14,0">views/scrollbar/scrollbar_horizontal.png</texturesliderbarfocus>
				<textureslidernib></textureslidernib>
				<textureslidernibfocus></textureslidernibfocus>
				<onup>51</onup>
				<ondown>9555</ondown>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<animation effect="fade" start="100" end="0" time="250" condition="!Control.HasFocus(60562) + !Container.Scrolling">Conditional</animation>
			</control>
		</control>
	</include>




